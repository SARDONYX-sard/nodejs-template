{
  "name": "nodejs-template",
  "private": "true",
  "version": "0.1.0",
  "description": "nodejs-template",
  "keywords": [],
  "homepage": "https://github.com/SARDONYX-sard/nodejs-template",
  "bugs": "https://github.com/SARDONYX-sard/nodejs-template/issues",
  "license": "MIT",
  "author": "",
  "files": [
    "dist",
    "./.github/CHANGELOG.md",
    "./.github/CODE_OF_CONDUCT.md",
    "./.github/LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SARDONYX-sard/nodejs-template"
  },
  "main": "build/src/main.js",
  "scripts": {
    "preinstall": "husky install && npx only-allow pnpm -y",
